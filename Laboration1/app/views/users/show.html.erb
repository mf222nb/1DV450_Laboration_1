<% if current_user && @key %>
    <h3>API Key:</h3>
    <%= link_to "Delete", @user, method: :delete %>

    <% elsif current_user.admin? %>
    <ul>
        <% @all_users.each do |i| %>

            <li><%= i.user.name %>, <%= i.key %><%= link_to "Delete", i.user, method: :delete %></li>
        <% end %>
    </ul>
<% end %>

